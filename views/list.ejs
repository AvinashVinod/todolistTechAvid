<%- include("header") -%>

<div class="box" id="heading">  
<h1><%= listTitle %></h1>
</div>  

<div class="box">

   <% for(var i=0;i<newItems.length;i++){ %>
    <div class="item">
       <input type="checkbox" name="checkbox">
       <p><%= newItems[i] %></p>
    </div>   
   <% }; %>   

<form class="item" method="post" action="/">

    <input type="text" name="content" placeholder="New Item" autocomplete="off"/>
    <button type="submit" name="button" value=<%= listTitle %>>+</button>

</form>

</div>

<%- include("footer") -%>
   